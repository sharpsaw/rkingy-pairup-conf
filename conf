# This is the top level set of setup actions
function pairup_setup {
    pairup_known_hosts
    pairup_ssh_config
    pairup_dots_install
    pairup_touch_sudo
#    pairup_apt_get BASE
#    pairup_apt_get RKING
    pairup_apt_get ALL
    pairup_man_db
    pairup_usr_local
    pairup_git_repos
    pairup_git_repos ACMEISM
#    pairup_vim_boot
}

##
# The rest of this file is variables:
##

# Git repos to clone
export PAIRUP_APT_GET='
git@github.com:ingydotnet/pairup.git
'

export PAIRUP_APT_GET_ACMEISM="\
git@github.com:ingydotnet/cdent.git
git@github.com:ingydotnet/pegex-pm.git
git@github.com:ingydotnet/testml-pm.git
git@github.com:ingydotnet/pegex-pgx.git
git@github.com:ingydotnet/json-pgx.git
git@github.com:ingydotnet/pegex-json-pm.git
"

# ... source repo
export PAIRUP_DOTDOTDOT_REPO=git@github.com:ingydotnet/....git

# .../conf contents
export PAIRUP_DOTS_CONF="\
dots:
- repo: git@github.com:sharpsaw/loop-dots.git
